{
  "name": "@flex-development/github",
  "description": "",
  "version": "0.0.0",
  "keywords": [],
  "homepage": "https://github.com/flex-development/.github",
  "repository": "https://github.com/flex-development/.github.git",
  "bugs": "https://github.com/flex-development/.github/issues",
  "author": {
    "name": "Lexus Drumgold",
    "url": "https://github.com/unicornware"
  },
  "type": "module",
  "scripts": {
    "check:ci": "yarn dedupe --check && yarn check:format && yarn check:lint && yarn check:spelling",
    "check:format": "prettier --check .",
    "check:lint": "eslint --exit-on-fatal-error --max-warnings 0 .",
    "check:spelling": "cspell lint --color --no-progress --relative $@ \"**\"",
    "check:upgrades": "yarn upgrade-interactive",
    "clean:modules": "trash ./.yarn/{cache,*.gz} ./node_modules",
    "commitlint": "cross-env TS_NODE_PROJECT=tsconfig.cjs.json commitlint -V",
    "fix:cg": "yarn fix:format && yarn fix:lint",
    "fix:dedupe": "yarn dedupe --strategy=highest",
    "fix:format": "prettier --cache --write .",
    "fix:lint": "yarn check:lint --cache --fix",
    "postinstall": "[ -f ./node_modules/.bin/husky ] && chmod +x .husky/* && husky install || exit 0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/types": "17.4.4",
    "@flex-development/commitlint-config": "1.0.1",
    "@graphql-eslint/eslint-plugin": "3.20.1",
    "@types/eslint": "8.44.2",
    "@types/node": "20.4.8",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "6.4.0",
    "cross-env": "7.0.3",
    "cspell": "7.0.1-alpha.9",
    "eslint": "8.47.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-jsdoc": "46.4.6",
    "eslint-plugin-jsonc": "2.9.0",
    "eslint-plugin-markdown": "3.0.1",
    "eslint-plugin-markdownlint": "0.5.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-unicorn": "48.0.1",
    "eslint-plugin-yml": "1.8.0",
    "graphql": "16.8.0",
    "graphql-config": "5.0.2",
    "husky": "8.0.3",
    "jsonc-eslint-parser": "2.3.0",
    "lint-staged": "14.0.0",
    "prettier": "3.0.2",
    "prettier-plugin-sh": "0.13.1",
    "trash-cli": "5.0.0",
    "typescript": "5.1.6",
    "yaml-eslint-parser": "1.2.2"
  },
  "engines": {
    "yarn": "4.0.0-rc.39"
  },
  "packageManager": "yarn@4.0.0-rc.39"
}
